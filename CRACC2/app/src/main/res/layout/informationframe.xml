<?xml version="1.0" encoding="utf-8"?>
<android.support.percent.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <FrameLayout
        android:id="@+id/informationframe"
        android:layout_width="match_parent"
        app:layout_heightPercent="80%"
        app:layout_marginTopPercent="13%"
        android:onClick="empty">

        <android.support.percent.PercentRelativeLayout
            android:id="@+id/informationlayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <android.support.percent.PercentRelativeLayout
                android:id="@+id/informationlayouttop"
                android:layout_width="match_parent"
                android:layout_alignParentTop="true"
                android:background="@drawable/informationboardtop"
                app:layout_heightPercent="22%">
                <android.support.percent.PercentRelativeLayout
                    android:id="@+id/informationlayouttopbutton"
                    android:layout_width="match_parent"
                    app:layout_marginTopPercent="3%"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/informationboardtop"
                    app:layout_heightPercent="60%">
                    <Button
                        android:id="@+id/informationiconbackground"
                        android:background="@drawable/roundiconcyan"
                        android:layout_centerInParent="true"
                        android:textColor="#000000"
                        app:layout_heightPercent="95%"
                        app:layout_aspectRatio="100%"
                        />
                <Button
                    android:id="@+id/informationicon"
                    android:background="@drawable/roundiconwhite"
                    android:layout_centerInParent="true"
                    android:textColor="#000000"
                    app:layout_heightPercent="84%"
                    app:layout_aspectRatio="100%"
                     />
                    </android.support.percent.PercentRelativeLayout>
                <TextView
                    android:id="@+id/informationsetting"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    app:layout_marginRightPercent="4%"
                    android:text="@string/setting"
                    android:textColor="#000000"
                    android:textSize="22sp"
                    android:onClick="inforamtionsetting"/>

                <TextView
                    android:id="@+id/informationtext"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:layout_below="@id/informationlayouttopbutton"
                    android:text="@string/name"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    app:layout_marginLeftPercent="33%"
                     />


            </android.support.percent.PercentRelativeLayout>

            <android.support.percent.PercentRelativeLayout
                android:id="@+id/informationlayoutbottom"
                android:layout_width="match_parent"
                android:layout_alignParentBottom="true"
                android:background="@drawable/informaitonboardwhite"
                app:layout_heightPercent="80%">

                <android.support.percent.PercentRelativeLayout
                    android:id="@+id/starlayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    app:layout_heightPercent="20%">

                    <Button
                        android:id="@+id/star1"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/star2"
                        android:textColor="#000000"
                        app:layout_heightPercent="50%"
                        app:layout_marginLeftPercent="15%"
                        app:layout_marginTopPercent="10%"
                        app:layout_widthPercent="12%"
                        android:onClick="star"/>

                    <Button
                        android:id="@+id/star2"
                        android:layout_alignParentTop="true"
                        android:layout_toEndOf="@id/star1"
                        android:background="@drawable/star2"
                        android:textColor="#000000"
                        app:layout_heightPercent="50%"
                        app:layout_marginLeftPercent="2.5%"
                        app:layout_marginTopPercent="10%"
                        app:layout_widthPercent="12%"
                        android:onClick="star"/>

                    <Button
                        android:id="@+id/star3"
                        android:layout_alignParentTop="true"
                        android:layout_toEndOf="@id/star2"
                        android:background="@drawable/star2"
                        android:textColor="#000000"
                        app:layout_heightPercent="50%"
                        app:layout_marginLeftPercent="2.5%"
                        app:layout_marginTopPercent="10%"
                        app:layout_widthPercent="12%"
                        android:onClick="star"/>

                    <Button
                        android:id="@+id/star4"
                        android:layout_alignParentTop="true"
                        android:layout_toEndOf="@id/star3"
                        android:background="@drawable/star2"
                        android:textColor="#000000"
                        app:layout_heightPercent="50%"
                        app:layout_marginLeftPercent="2.5%"
                        app:layout_marginTopPercent="10%"
                        app:layout_widthPercent="12%"
                        android:onClick="star"/>

                    <Button
                        android:id="@+id/star5"
                        android:layout_alignParentTop="true"
                        android:layout_toEndOf="@id/star4"
                        android:background="@drawable/star2"
                        android:textColor="#000000"
                        app:layout_heightPercent="50%"
                        app:layout_marginLeftPercent="2.5%"
                        app:layout_marginTopPercent="10%"
                        app:layout_widthPercent="12%"
                        android:onClick="star"/>

                </android.support.percent.PercentRelativeLayout>

                <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:id="@+id/informationcommentlayout"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/starlayout"
                    app:layout_marginTopPercent="1%"

                    app:layout_widthPercent="50%">

                    <TextView
                        android:id="@+id/age"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="5sp"
                        android:gravity="end"
                        android:text="@string/age"
                        android:textColor="#000000"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/lastgame"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/age"
                        android:layout_marginTop="5sp"
                        android:gravity="end"
                        android:text="@string/lastgame"
                        android:textColor="#000000"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/play"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/lastgame"
                        android:layout_marginTop="5sp"
                        android:gravity="end"
                        android:text="@string/play"
                        android:textColor="#000000"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/canceled"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/play"
                        android:layout_marginTop="5sp"
                        android:gravity="end"
                        android:text="@string/canceled"
                        android:textColor="#000000"
                        android:textSize="20sp" />


                </RelativeLayout>



            </android.support.percent.PercentRelativeLayout>

        <android.support.percent.PercentRelativeLayout
            android:id="@+id/informationcommentlayoutafter"
            android:layout_height="27sp"
            android:layout_width="match_parent"
            app:layout_marginTopPercent="65%"
            >

                <TextView
                    android:id="@+id/comment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="@string/comment"
                    android:textColor="#4C04DC"
                    android:textSize="22sp" />
            <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/informationcommentline1"
                android:background="#000000"
                android:layout_height="2sp"
                app:layout_widthPercent="30%"
                android:layout_toEndOf="@+id/comment"
                app:layout_marginLeftPercent="2%"
                android:layout_centerVertical="true"
                />
            <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/informationcommentline2"
                android:background="#000000"
                android:layout_height="2sp"
                app:layout_widthPercent="30%"
                android:layout_toStartOf="@+id/comment"
                app:layout_marginRightPercent="2%"
                android:layout_centerVertical="true"
                />

            </android.support.percent.PercentRelativeLayout>

        </android.support.percent.PercentRelativeLayout>
    </FrameLayout>


</android.support.percent.PercentRelativeLayout>